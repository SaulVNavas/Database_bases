        "Arduino": {
            "hasTokens": false,
            "tokenConfig": null,
            "tokenRules": null
        },
        "Micropython": {
            "hasTokens": false,
            "tokenConfig": null,
            "tokenRules": null
        }
    },
    "decorations": {
        "Vhdl": [
            {
                "regex": "(?<![a-zA-Z])\\d*\\.?\\d+(?![a-zA-Z])",
                "style": "vhdl-numbers-style"
            },
            {
                "regex": "(?<![a-zA-Z0-9])'[^']*'(?![a-zA-Z0-9])",
                "style": "vhdl-naming-style"
            }
        ],
        "Micropython": [
            {
                "regex": "machine|network|utime|uos|I2C|SPI|ADC|PWM",
                "style": "micropyhton-keywords-style"
            }
        ],
        "Arduino": [
            {
            "regex": "setup|loop|pinMode|digitalWrite|digitalRead|analogWrite|analogRead|delay",
            "style": "arduino-keywords-style"
            }
        ]
    },
    "baseLanguages": {
        "Arduino": "c",
        "Micropython": "python",
        "Vhdl": "vhdl"
    }
